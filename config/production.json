{
  "host": "itemized-mirandaflu.herokuapp.com/",
  "port": "PORT",
  "mongodb": "MONGODB",
  "public": "../app/",
  "auth": {
    "token": {
      "secret": "FEATHERS_AUTH_SECRET"
    },
    "google": {
      "clientID": "GOOGLE_OAUTH_CLIENT_ID",
      "clientSecret": "GOOGLE_OAUTH_CLIENT_SECRET",
      "permissions": {
        "authType": "rerequest",
        "scope": ["profile"]
      }
    },
    "successRedirect": "/",
    "shouldSetupSuccessRoute": false,
    "failureRedirect": "/login?error=auth",
    "shouldSetupFailureRoute": false,
    "failureFlash": true
  },
  "s3": {
    "accessKeyId": "S3_ACCESS_ID",
    "secretAccessKey": "S3_ACCESS_KEY",
    "bucket": "S3_BUCKET"
  }
}
